<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="privateMessageService" class="top.yein.tethys.service.PrivateMessageServiceImpl">
    <constructor-arg name="privateMessageRepository" ref="privateMessageRepository"/>
  </bean>
  <bean id="groupMessageService" class="top.yein.tethys.service.GroupMessageServiceImpl">
    <constructor-arg name="groupMessageRepository" ref="groupMessageRepository"/>
  </bean>

</beans>